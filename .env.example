# ===================================
# JOB AGENT - ENVIRONMENT CONFIGURATION
# ===================================
# Copy this file to .env and fill in your actual values
# NEVER commit the .env file - it contains sensitive information!

# ===================================
# REQUIRED: API CREDENTIALS
# ===================================

# Google Gemini API Key (REQUIRED for AI features)
# Get your free API key: https://aistudio.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# ===================================
# REQUIRED: PERSONAL INFORMATION
# ===================================

# Your Full Name
YOUR_NAME=John Doe

# First and Last Name (used in form filling)
FIRST_NAME=John
LAST_NAME=Doe

# Contact Information
YOUR_EMAIL=your.email@example.com
YOUR_PHONE=5551234567

# Physical Address
STREET_ADDRESS=123 Main Street
CITY=New York
STATE=New York
ZIP_CODE=10001

# Location (as you'd like it to appear on resume)
LOCATION=New York, NY

# ===================================
# REQUIRED: PROFESSIONAL PROFILES
# ===================================

# LinkedIn Profile URL
YOUR_LINKEDIN_PROFILE_URL=https://linkedin.com/in/your-username

# GitHub Profile URL
YOUR_GITHUB_URL=https://github.com/your-username

# Portfolio/Website (optional)
WEBSITE=https://your-portfolio.com

# LeetCode Profile (optional)
YOUR_LEETCODE_URL=

# ===================================
# REQUIRED: WORK AUTHORIZATION
# ===================================

# Are you authorized to work in the US?
# Options: Yes, No
WORK_AUTHORIZED=Yes

# Do you require visa sponsorship?
# Options: Yes, No, Now, In the future
REQUIRE_SPONSORSHIP=No

# ===================================
# REQUIRED: EDUCATION
# ===================================

# Your highest or most relevant degree
UNIVERSITY=New York University
DEGREE=Master of Science in Computer Science
EDUCATION_LOCATION=New York, NY
EDUCATION_DATES=Jan 2020 -- May 2024

# ===================================
# REQUIRED: PROFESSIONAL BACKGROUND
# (Used for AI-generated answers to application questions)
# ===================================

# Years of professional experience
YEARS_EXPERIENCE=3

# Current or most recent job title
JOB_TITLE_CURRENT=Software Engineer

# Your primary tech stack (comma-separated)
TECH_STACK=Python, JavaScript, React, Node.js, Docker, AWS

# Key achievement or impact statement
KEY_ACHIEVEMENT=Built scalable web applications serving 100K+ users

# Your specializations or focus areas
SPECIALIZATIONS=Full-stack development, cloud architecture, automation

# Soft skills and strengths
SOFT_SKILLS=Strong problem-solving abilities, quick learner, excellent communicator

# What drives you in your career?
CAREER_PASSION=building efficient, user-friendly solutions that solve real problems

# ===================================
# OPTIONAL: DEMOGRAPHIC INFORMATION
# ===================================
# Used for EEO (Equal Employment Opportunity) forms
# You can set these to "Prefer not to say" if you wish

GENDER=Prefer not to say
RACE_ETHNICITY=Prefer not to say
VETERAN_STATUS=I am not a protected veteran
DISABILITY_STATUS=I do not have a disability

# ===================================
# OPTIONAL: JOB SCRAPING CREDENTIALS
# ===================================
# Only needed if you want to scrape jobs from these platforms

# LinkedIn Session Cookie (li_at cookie value)
# Instructions: Login to LinkedIn, open DevTools > Application > Cookies > linkedin.com > find 'li_at' cookie
LINKEDIN_LI_AT_COOKIE=

# JobRight Cookie String
# Instructions: Login to JobRight, open DevTools > Application > Cookies > copy all cookies
JOBRIGHT_COOKIE_STRING=

# ===================================
# OPTIONAL: DATABASE CONFIGURATION
# ===================================

# MongoDB Connection String
# Default uses local MongoDB instance
MONGODB_CONNECTION_STRING=mongodb://localhost:27017/

# Database Name
DB_NAME=job_agent_db

# ===================================
# OPTIONAL: AI CONFIGURATION
# ===================================

# Gemini Model Name
# Options: gemini-2.5-flash-lite (recommended - fast, high rate limits)
#          gemini-1.5-flash (balanced)
#          gemini-1.5-pro (most capable but slower)
GEMINI_MODEL_NAME=gemini-2.5-flash-lite

# ===================================
# OPTIONAL: APPLICATION BEHAVIOR
# ===================================

# Maximum number of jobs to process per run
MAX_JOBS_TO_PROCESS_PER_RUN=5

# Delay between applications (in seconds) to appear more human-like
# Set to 0 for testing, 30-90 for production use
APPLICATION_DELAY_MIN=0
APPLICATION_DELAY_MAX=0

# Job scraping limit per platform
SCRAPER_JOB_LIMIT=25

# ===================================
# OPTIONAL: ADVANCED CONFIGURATION
# ===================================

# Logging level (DEBUG, INFO, WARNING, ERROR, CRITICAL)
LOG_LEVEL=INFO

# Output directories (relative to project root)
BASE_OUTPUT_DOCS_DIR=output_documents
PROCESSED_APPS_DIR=processed_applications

# PDFLaTeX path (auto-detected on macOS, specify if needed)
PDFLATEX_PATH=

# Chrome executable and driver paths (usually auto-detected)
CHROME_EXECUTABLE_PATH=
CHROME_DRIVER_PATH=

# Chrome User Data Directory (for browser session persistence)
# macOS: /Users/USERNAME/Library/Application Support/Google/Chrome
# Windows: C:\Users\USERNAME\AppData\Local\Google\Chrome\User Data
# Linux: /home/USERNAME/.config/google-chrome
CHROME_USER_DATA_DIR=

# Custom user agent for web scraping (usually not needed)
DEFAULT_USER_AGENT=

# JobRight API Configuration
JOBRIGHT_MAX_POSITION=100
JOBRIGHT_POSITION_INCREMENT=10
JOBRIGHT_REQUEST_DELAY_SECONDS=2.0

# ===================================
# OPTIONAL: ACCOUNT CREDENTIALS
# ===================================

# Generic password for creating accounts on job sites (if needed)
# SECURITY WARNING: Use a disposable password, NOT your regular passwords!
YOUR_PASSWORD=
